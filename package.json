{
  "name": "image-annotation",
  "version": "1.0.0",
  "description": "Anime image annotation system for ML training data using waifu.im API",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:github": "cross-env NEXT_PUBLIC_BASE_PATH=/image-annotation next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "deploy": "cross-env NEXT_PUBLIC_BASE_PATH=/image-annotation next build && gh-pages -d out"
  },
  "dependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "24.8.1",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0"
  }
}
